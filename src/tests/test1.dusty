program real_test;

vars
  length, width, area:int;

void calculate_area(length: int, width: int) {
  area = length * width;
  if (area > 30) {
    print!("Big Area!");
  } else {
    print!("Small Area!");
  };
  print!("something else");
};

begin
{
  length = 5;
  width = 1;
  calculate_area(length, width);
  print!(area);
  if (area > 40) {
    print!("huge area!");
  } else {
    print!("not so huge area...");
  };
  print!("something else!!");
}
end