program real_test2;

vars example:int;

void print_some(ex: int) {
  while (ex > 0) do {
    print!(ex);
    ex = ex - 1;
  };
  print!("Hello");
};

begin
{
  if (10 < 1) {
    print!("whoopsie!");
  } else {
    print!("obviously!");
    if (4 > 1) {
      print!("another obvious one!");
    } else {
      print!("another whoopsie!");
    };
  };
  example = 5;
  print_some(example);
}
end